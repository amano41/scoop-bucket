    "homepage": "https://winmergejp.bitbucket.io/",
    "description": "An open source differencing and merging tool for Windows that compares both folders and files.",
    "license": "GPL-2.0-only",
    "version": "2.16.6+-jp-11",
    "architecture": {
        "32bit": {
            "url": "https://osdn.net/frs/redir.php?f=winmerge-jp%2F73388%2Fwinmerge-2.16.6-jp-11-exe.zip#/dl.7z",
            "hash": "eb4b8a4d8ad52d7ac112322eb10f1acd0108d3df726479bcd8b8eac8f21ce6aa"
        },
        "64bit": {
            "url": "https://osdn.net/frs/redir.php?f=winmerge-jp%2F73388%2Fwinmerge-2.16.6-jp-11-x64-exe.zip#/dl.7z",
            "hash": "3d96be76cab287e3ce1bb30f643bcbff638d21c033fedc8d1bb926dbdfc8377b"
        }
    },
    "extract_dir": "WinMerge",
