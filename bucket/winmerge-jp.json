    "shortcuts": [
        [
            "WinMergeU.exe",
            "WinMerge-JP"
        ]
    ],
    "persist": "MergePlugins",
    "checkver": {
        "url": "https://osdn.net/projects/winmerge-jp/releases/",
        "re": "downloads/(?<release>[\\d]{5})/WinMerge-(?<version>[\\d.]+)-jp-(?<build>[\\d]+)-x64-Setup.exe",
