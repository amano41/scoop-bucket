{
    "homepage": "https://winmergejp.bitbucket.io/",
    "description": "An open source differencing and merging tool for Windows that compares both folders and files.",
    "license": "GPL-2.0-only",
    "version": "2.16.34-jp-1",
    "architecture": {
        "32bit": {
            "url": "https://download.sourceforge.net/winmerge-v2-jp/winmerge-2.16.34-jp-1-exe.zip#/dl.7z",
            "hash": "6b7025276c04cfc9280446196fc4399ed8105482ade17d8377665d83f3ab0d5e"
        },
        "64bit": {
            "url": "https://download.sourceforge.net/winmerge-v2-jp/winmerge-2.16.34-jp-1-x64-exe.zip#/dl.7z",
            "hash": "3c60957b1f92dea2aef0a3304dab6d900557fff79f3f39c94288e49e261a51d1"
        }
    },
    "extract_dir": "WinMerge",
    "bin": "WinMergeU.exe",
    "shortcuts": [
        [
            "WinMergeU.exe",
            "WinMerge-JP"
        ]
    ],
    "persist": "MergePlugins",
    "checkver": {
        "url": "https://winmergejp.bitbucket.io/",
        "re": "<h3>\\d{4}/\\d{2}/\\d{2} (?<version>[\\d.]+)\\-jp-(?<build>[\\d]+)</h3>",
        "replace": "${version}-jp-${build}"
    },
    "autoupdate": {
        "architecture": {
            "32bit": {
                "url": "https://download.sourceforge.net/winmerge-v2-jp/winmerge-$matchVersion-jp-$matchBuild-exe.zip#/dl.7z"
            },
            "64bit": {
                "url": "https://download.sourceforge.net/winmerge-v2-jp/winmerge-$matchVersion-jp-$matchBuild-x64-exe.zip#/dl.7z"
            }
        }
    }
}
